<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/iv_bot"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/matching_bot"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ai_matching_result_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:fontFamily="@font/pretendardbold"
        android:gravity="start"
        android:text="@string/ai_matching_result_title"
        android:textColor="@color/olive_black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/iv_bot"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_bot"
        app:layout_constraintTop_toTopOf="@+id/iv_bot" />

    <TextView
        android:id="@+id/ai_matching_result_description"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="15dp"
        android:fontFamily="@font/pretendardregular"
        android:gravity="start"
        android:text="@string/ai_matching_result_description"
        android:textColor="@color/gray2"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ai_matching_result_title" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_matching_result_dev_info1"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:background="@drawable/dialog_button_background_main_olive"
        android:paddingHorizontal="15dp"
        android:paddingVertical="20dp"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ai_matching_result_description">


        <TextView
            android:id="@+id/tv_developer_name1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10dp"
            android:fontFamily="@font/pretendardbold"
            android:gravity="start"
            android:textColor="@color/olive_black"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Name" />

        <TextView
            android:id="@+id/tv_developer_email1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="2dp"
            android:layout_marginTop="5dp"
            android:ellipsize="end"
            android:fontFamily="@font/pretendardmedium"
            android:gravity="center"
            android:maxLines="2"
            android:textColor="@color/white"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_developer_name"
            tools:text="yuri1231231231ssss23@naver.com" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_matching_result_dev_info2"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:background="@drawable/dialog_button_background_main_olive"
        android:paddingHorizontal="15dp"
        android:paddingVertical="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cl_matching_result_dev_info1">


        <TextView
            android:id="@+id/tv_developer_name2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10dp"
            android:fontFamily="@font/pretendardbold"
            android:gravity="start"
            android:textColor="@color/olive_black"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Name" />

        <TextView
            android:id="@+id/tv_developer_email2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="2dp"
            android:layout_marginTop="5dp"
            android:ellipsize="end"
            android:fontFamily="@font/pretendardmedium"
            android:gravity="center"
            android:maxLines="2"
            android:textColor="@color/white"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_developer_name"
            tools:text="yuri1231231231ssss23@naver.com" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_matching_result_dev_info3"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:background="@drawable/dialog_button_background_main_olive"
        android:paddingHorizontal="15dp"
        android:paddingVertical="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cl_matching_result_dev_info2">


        <TextView
            android:id="@+id/tv_developer_name3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10dp"
            android:fontFamily="@font/pretendardbold"
            android:gravity="start"
            android:textColor="@color/olive_black"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Name" />

        <TextView
            android:id="@+id/tv_developer_email3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="2dp"
            android:layout_marginTop="5dp"
            android:ellipsize="end"
            android:fontFamily="@font/pretendardmedium"
            android:gravity="center"
            android:maxLines="2"
            android:textColor="@color/white"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_developer_name"
            tools:text="yuri1231231231ssss23@naver.com" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

