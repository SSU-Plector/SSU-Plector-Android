            android:gravity="center"
            android:maxLines="1"
            android:textColor="@color/gray3"
            android:textSize="10sp"
            app:layout_constraintStart_toStartOf="@id/tv_developer1_part1"
            app:layout_constraintTop_toBottomOf="@+id/tv_developer1_part1"
            tools:text="yuri1231231231ssss23@naver.com" />

        <TextView
            android:id="@+id/tv_developer1_part1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="20dp"
            android:fontFamily="@font/pretendardsemibold"
            android:textColor="@color/main_olive"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_developer_name1"
            app:layout_constraintStart_toEndOf="@+id/tv_developer_name1"
            app:layout_constraintTop_toTopOf="@id/tv_developer_name1"
            tools:text="service" />

